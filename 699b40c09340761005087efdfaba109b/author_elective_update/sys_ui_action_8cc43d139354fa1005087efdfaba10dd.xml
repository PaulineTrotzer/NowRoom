        <name>Cancel Reservation (Server)</name>
        <onclick/>
        <order>300</order>
        <script><![CDATA[try {
  // Debug sichtbar machen
  gs.addInfoMessage('SERVER HIT: nowroom_cancel_reservation');

  if (!current || !current.getUniqueValue()) {
    gs.addErrorMessage('Record not found.');
    action.setAbortAction(true);
    action.setRedirectURL(current);
  } else {
    // Feld ermitteln
    var fieldName = current.isValidField('reservation_status') ? 'reservation_status'
                  : current.isValidField('status') ? 'status'
                  : null;

    if (!fieldName) {
      gs.addErrorMessage('No status field (reservation_status/status) found.');
      action.setAbortAction(true);
      action.setRedirectURL(current);
    } else {
      // (Guards bei Bedarf wieder aktivieren)
      // if (current.getValue('opened_by') != gs.getUserID()) {
      //   gs.addErrorMessage('You are not allowed to cancel this reservation.');
      //   action.setAbortAction(true);
      //   action.setRedirectURL(current);
      // } else {

      var oldVal = String(current.getValue(fieldName) || '');
      gs.addInfoMessage('DEBUG: current ' + fieldName + ' = ' + oldVal);

      // Internal Choice-Value setzen (anpassen falls anders)
      current.setValue(fieldName, 'cancelled');

      // Fallback: per Display-Value (z. B. "Cancelled")
      if (String(current.getValue(fieldName) || '') != 'cancelled') {
        current.setDisplayValue(fieldName, 'Cancelled');
      }

      var upd = current.update();
      gs.addInfoMessage('DEBUG: update id = ' + upd);

      var newVal = String(current.getValue(fieldName) || '');
      gs.addInfoMessage('DEBUG: new ' + fieldName + ' = ' + newVal);

      if ((newVal || '').toLowerCase() == 'cancelled') {
        gs.addInfoMessage('Reservation has been cancelled.');
      } else {
        gs.addErrorMessage('Could not set to "cancelled" — check Choice internal value / ACL / BR/Flows.');
      }

      action.setRedirectURL(current);
      // }
    }
  }
} catch (e) {
  gs.error('Cancel UI Action error: ' + e);
  gs.addErrorMessage('Unable to cancel reservation: ' + (e && e.message ? e.message : e));
  action.setAbortAction(true);
  action.setRedirectURL(current);
}
]]></script>
