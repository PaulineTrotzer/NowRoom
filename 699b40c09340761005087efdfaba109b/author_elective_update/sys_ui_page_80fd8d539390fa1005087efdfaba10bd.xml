        <html><![CDATA[<div style="padding:16px;max-width:520px">
  <h2 style="margin:0 0 8px;">Cancel reservation?</h2>
  <p style="margin:0 0 16px;">Do you really want to cancel this reservation?</p>

  <div style="display:flex;gap:8px;justify-content:flex-end">
    <!-- Cancel -->
    <button type="button" class="btn btn-default"
      onclick="
        try {
          if (typeof GlideDialogWindow !== 'undefined') {
            var d = (GlideDialogWindow.get) ? GlideDialogWindow.get() : null;
            if (d) { d.destroy(); }
          }
        } catch(e) {}
        try {
          if (typeof GlideModal !== 'undefined') {
            var m = (GlideModal.get) ? GlideModal.get() : null;
            if (m) { m.destroy(); }
          }
        } catch(e) {}
      ">
      Cancel
    </button>

    <!-- OK -->
    <button type="button" class="btn btn-danger"
      onclick="
        try {
          var tw = (typeof getTopWindow === 'function') ? getTopWindow() : ((typeof top !== 'undefined') ? top : self);
          if (tw) {
            if (tw.g_form) {
              tw.g_form.submit('cancel_reservation');  /* Action name deiner UI Action */
            } else if (typeof g_form !== 'undefined') {
              g_form.submit('cancel_reservation');
            }
          }
        } catch(e) {}
        try {
          if (typeof GlideDialogWindow !== 'undefined') {
            var d2 = (GlideDialogWindow.get) ? GlideDialogWindow.get() : null;
            if (d2) { d2.destroy(); }
          }
        } catch(e) {}
        try {
          if (typeof GlideModal !== 'undefined') {
            var m2 = (GlideModal.get) ? GlideModal.get() : null;
            if (m2) { m2.destroy(); }
          }
        } catch(e) {}
      ">
      OK
    </button>
  </div>
</div>
]]></html>
