        <script_false><![CDATA[function onCondition() {
	g_form.hideFieldMsg('planned_end', true);
}]]></script_false>
        <script_true><![CDATA[function onCondition() {
	var s = g_form.getValue('planned_start');
	var e = g_form.getValue('planned_end');

	

	if (!s || !e) return;

	var start = new Date(s.replace(" ", "T"));
	var end = new Date(e.replace(" ", "T"));

	if (end <= start) {
		g_form.showFieldMsg('planned_end', 'Planned End must be after Planned Start', 'error');
	}
}
]]></script_true>
