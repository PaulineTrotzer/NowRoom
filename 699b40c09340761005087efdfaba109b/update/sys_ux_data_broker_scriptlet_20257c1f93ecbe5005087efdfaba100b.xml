    <sys_ux_data_broker_scriptlet action="INSERT_OR_UPDATE">
        <api_name>x_1782765_nowroom.ui_events_scriptlet</api_name>
        <batch_exclude>false</batch_exclude>
        <description/>
        <mutates_server_data>false</mutates_server_data>
        <name>ui_events_scriptlet</name>
        <output_schema>{&#13;
  "type": "array",&#13;
  "items": {&#13;
    "type": "object",&#13;
    "properties": {&#13;
      "id":    { "type": "string" },&#13;
      "title": { "type": "string" },&#13;
      "start": { "type": "string" },&#13;
      "end":   { "type": "string" }&#13;
    },&#13;
    "required": ["id","title","start","end"]&#13;
  }&#13;
}&#13;
</output_schema>
        <private>false</private>
        <props/>
        <required_translations>[ ]</required_translations>
        <schema_version>1.0.0</schema_version>
        <script><![CDATA[function handler() {
  var items = [];
  var gr = new GlideRecord('x_1782765_nowroom_ui_events');
  gr.orderBy('start');
  gr.query();
  while (gr.next()) {
    var s = gr.getValue('start');
    var e = gr.getValue('end');
    if (!s || !e) continue;
    items.push({
      id: gr.getUniqueValue(),
      title: gr.getValue('title') || 'Event',
      start: s.replace(' ', 'T'),
      end:   e.replace(' ', 'T')
    });
  }
  return { output: items };
}
]]></script>
